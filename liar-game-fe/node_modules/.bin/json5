#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@surma/rollup-plugin-off-main-thread/node_modules/json5/lib/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../@surma/rollup-plugin-off-main-thread/node_modules/json5/lib/cli.js" "$@"
  ret=$?
fi
exit $ret
